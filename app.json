{
  "expo": {
    "name": "Triangl",
    "slug": "Triangl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "API_URL": "https://trianglpanel.com/api",
      "eas": {
        "projectId": "4546ba12-b737-499a-8fe9-d94c234ed15c"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundler": "metro",
      "infoPlist": {
        "NSCameraUsageDescription": "Aplikacija treba pristup kameri za skeniranje QR kodova"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "bundler": "metro"
      },
      "package": "com.crni89.myapp",
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "owner": "crni89"
  }
}
